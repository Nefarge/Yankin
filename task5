#include <iostream>
using namespace std;
double f1(double x);
void  main()
{
    int x = 5;
    double f, y1;
    f = (1 + sin(2 * x)) / cos(2 * x);
    cout.precision(4); //формат 
    cout << f;    //вывод


    cout << "\nvvedite x \n";
    cin >> x;
    f = (1 + sin(2 * x)) / cos(2 * x);
    cout << f;


    cout << "\nvvedite x \n";
    cin >> x;
    y1 = f1(x);
    cout << y1;
}


